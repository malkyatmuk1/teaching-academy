{
  "name": "teaching-academy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_via_ip": "ng serve --proxy-config proxy_via_ip.config.json --port 4200 -c development",
    "start_local": "ng serve --proxy-config proxy_via_localhost.config.json --port 4200 -c development",
    "build": "ng build",
    "build_gp": "ng build --aot --base-href https://malkyatmuk1.github.io/teaching-academy/",
    "publish_gp": "npx angular-cli-ghpages --dir=dist/teaching-academy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run teaching-academy:serve-ssr",
    "serve:ssr": "node dist/teaching-academy/server/main.js",
    "build:ssr": "ng build && ng run teaching-academy:server",
    "prerender": "ng run teaching-academy:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/platform-server": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/ssr": "^17.0.8",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "jquery": "^3.7.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "~17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^1.0.7",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.2.2"
  }
}